@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="padding1">
    <h4>En Beğendiğim yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" />
            </div>
            <div class="bot-desc">
                <p>@x.Baslik </p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")

</div>